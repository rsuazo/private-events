<h1>Create a New Event!</h1>

<%= form_with(model: @event) do %>

  <%= label_tag(:location, "Description:") %>
  <%= text_field(:event, :location) %>

  </br></br>

  <%= label_tag(:start_date, "Start Date:") %>
  <%= date_field(:event, :start_date) %>

  </br></br>

  <%= submit_tag("Create Event!") %>
  
<% end %>